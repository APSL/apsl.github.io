{
    "env": {
        "browser": true,
        "es6": true
    },
    "ecmaFeatures": {
        "jsx": true,
        "modules": true
    },
    "parser": "babel-eslint",
    "rules": {
        "strict": [2, "never"],
        "indent": [2, 2],
        "max-len": [2, 100, 2, {"ignoreUrls": true}]
    },
    "plugins": [
        "react"
    ]
}
